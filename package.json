    "html2canvas": "^1.0.0-alpha.12",
    "immutability-helper": "^2.6.4",
    "immutable": "^3.8.2",
    "jointjs": "^2.0.1",
    "konva": "^2.1.3",
    "lodash": "^4.17.5",
    "lodash.bindall": "^4.4.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
